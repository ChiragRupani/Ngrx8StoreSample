{
  "name": "todoapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --browsers=EdgeAnyHeadless",
    "lint": "ng lint",
    "migrate-cli": "ng update @angular/cli --from 20 --to 21 --migrate-only --allow-dirty",
